{
    "name": "apicloud-staging",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "start-wifi": "apicloud wifiStart --port 8686",
        "sync": "apicloud wifiSync --project ./ --updateAll false --port 8686",
        "sync-all": "apicloud wifiSync --project ./ --updateAll true --port 8686",
        "sync-dist": "apicloud wifiSync --project ./dist --updateAll true --port 8686",
        "cli": "ts-node --project ./cli/tsconfig.json ./cli/index.ts"
    },
    "dependencies": {
        "core-js": "^2.6.5",
        "dayjs": "^1.8.14",
        "vue": "^2.6.10",
        "vue-property-decorator": "^8.2.1"
    },
    "devDependencies": {
        "@types/fs-extra": "^8.0.0",
        "@types/inquirer": "^6.0.3",
        "@types/yargs": "^13.0.0",
        "@vue/cli-plugin-babel": "^3.8.0",
        "@vue/cli-plugin-eslint": "^3.8.0",
        "@vue/cli-plugin-typescript": "^3.8.0",
        "@vue/cli-service": "^3.8.0",
        "@vue/eslint-config-standard": "^4.0.0",
        "@vue/eslint-config-typescript": "^4.0.0",
        "babel-eslint": "^10.0.1",
        "colors": "^1.3.3",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^6.0.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-vue": "^5.0.0",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "fs-extra": "^8.1.0",
        "inquirer": "^6.4.1",
        "sass": "^1.18.0",
        "sass-loader": "^7.1.0",
        "shelljs": "^0.8.3",
        "ts-node": "^8.3.0",
        "typescript": "^3.4.3",
        "vue-template-compiler": "^2.6.10",
        "yargs": "^13.2.4"
    }
}
